{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (OpenOCD)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/Tools/blinkled.elf",
      "device": "STM32F103C8Tx",
      "configFiles": [
        "E:/OpenOCD/xpack-openocd-0.12.0-6/openocd/scripts/interface/stlink.cfg",
        "E:/OpenOCD/xpack-openocd-0.12.0-6/openocd/scripts/target/stm32f1x.cfg"
      ],
      "runToEntryPoint": "main",
      "preLaunchTask": "Build STM32",
      "svdFile": "${workspaceFolder}/STM32F103.svd"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "E:/AUTOSAR-Classic-ECU-Development/Project_FanControl_AUTOSAR",
      "program": "E:/AUTOSAR-Classic-ECU-Development/Project_FanControl_AUTOSAR/Tools/blinkled.elf",
      /* Ko cần thay đổi đường dẫn này nhưng vẫn chạy dúng, thực chất là file elf mới tạo ra ghi đè lên file của thư mục cũ*/
      /* Thay đổi đúng đường dẫn mới chạy được ngắt >ii<*/
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}